<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rootlab.practice.file.FMetaMapper">

<resultMap id="FMetaResultMap" type="com.rootlab.practice.file.FileMeta">
    <id property="id" column="id" />
    <result property="name" column="name" />
    <result property="size" column="size" />
    <result property="type" column="type" />
    <result property="createDateTime" column="createtime" />
    <result property="path" column="path" />
    <result property="link" column="link" />
    
</resultMap>

	<select id="selectAllFileMeta" resultMap="FMetaResultMap">
		SELECT * FROM filemeta;
	</select>


</mapper>